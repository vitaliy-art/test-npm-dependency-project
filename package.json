{
  "name": "varta",
  "author": "vldfokin",
  "version": "0.8.28",
  "description": "vertical embed rotator",
  "scripts": {
    "test:test": "cross-env START_SERVER_AND_TEST_INSECURE=1 start-server-and-test test:server https://localhost:9000 \"cypress run --browser chrome --spec 'cypress/integration/viewslimit.js'\"",
    "dev": "cross-env NODE_ENV=dev && gulp --gulpfile ./config/gulpfile.js && webpack-dev-server --open --config config/webpack.dev.js --env NODE_ENV=dev --env CHECK=true",
    "dev:settings": "cross-env NODE_ENV=dev gulp --gulpfile ./config/gulpfile.js & webpack-dev-server --open --config config/webpack.dev.js --env.NODE_ENV=localSettings",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "mochapack --timeout 5000 --webpack-config config/webpack.test.js \"mocha/*.js\"",
    "test:prod": "mochapack --timeout 5000 --webpack-config config/webpack.test.js \"mocha/prod/*.js\"",
    "test:server": "cross-env NODE_ENV=dev gulp --gulpfile ./config/gulpfile.js & webpack-dev-server --config config/webpack.test-server.js",
    "test:e2e": "cross-env START_SERVER_AND_TEST_INSECURE=1 start-server-and-test test:server https://localhost:9000 \"cypress run --browser chrome\"",
    "build": "rimraf ./dist && mkdirp ./dist && cross-env NODE_ENV=prod gulp html --gulpfile ./config/gulpfile.js && webpack --env.NODE_ENV=prod --config config/webpack.prod.js && webpack --env.NODE_ENV=prod --config config/webpack.prod-debug.js",
    "build:mea": "rimraf ./dist && mkdirp ./dist && cross-env NODE_ENV=prod gulp html --gulpfile ./config/gulpfile.js && webpack --env.NODE_ENV=prod --env.CHECK=true --config config/webpack.prod.js && webpack --env.NODE_ENV=prod --config config/webpack.prod-debug.js",
    "deploy": "scp -rp ./dist/* vertel@blaine.kost.tv:~/static.kost.tv/varta/releases && VERSION=$(node -p \"require('./package.json').version\") && ssh vertel@blaine.kost.tv \"ln -fs ~/static.kost.tv/varta/releases/$VERSION/ ~/static.kost.tv/public_html/$VERSION\"",
    "deploysdn": "scp -rp ./dist/* kosttv@origin.sdn.kstdn.tech:~/varta/releases && VERSION=$(node -p \"require('./package.json').version\") && ssh kosttv@origin.sdn.kstdn.tech \"ln -fs ~/varta/releases/$VERSION ~/static.kost.tv/$VERSION\"",
    "production": "npm run test && npm run build && npm run test:prod && npm run deploy",
    "productionsdn": "npm run test && npm run build && npm run test:prod && npm run deploysdn",
    "release:current": "VERSION=$(node -p \"require('./package.json').version\") && ssh vertel@blaine.kost.tv \"rm ~/static.kost.tv/public_html/current && ln -s ~/static.kost.tv/varta/releases/$VERSION ~/static.kost.tv/public_html/current\"",
    "releasesdn:current": "VERSION=$(node -p \"require('./package.json').version\") && ssh kosttv@origin.sdn.kstdn.tech \"rm ~/static.kost.tv/current && ln -s ~/varta/releases/$VERSION ~/static.kost.tv/current\"",
    "build-stage": "rimraf ./dist && mkdirp ./dist && cross-env NODE_ENV=stage gulp html --gulpfile ./config/gulpfile.js && webpack --env.NODE_ENV=stage --config config/webpack.prod.js && webpack --env.NODE_ENV=stage --config config/webpack.prod-debug.js",
    "deploy-stage": "scp -rp ./dist/* vertel@stage.kost.tv:~/stage.static.kost.tv/varta/releases && VERSION=$(node -p \"require('./package.json').version\") && ssh vertel@stage.kost.tv \"ln -fs ~/stage.static.kost.tv/varta/releases/$VERSION/ ~/stage.static.kost.tv/public_html/$VERSION\"",
    "deploy-stage2": "scp -rp ./dist/* kkoritich@dev.kost.tv:~/dev.kost.tv/public_html/utils/varta/releases && cross-env-file -p ./package.json && ssh kkoritich@dev.kost.tv \"ln -fs ~/dev.kost.tv/varta/releases/$VERSION/ ~/dev.kost.tv/public_html/$VERSION\"",
    "stage": "npm ci && npm run test && npm run build-stage && npm run deploy-stage",
    "release-stage:current": "VERSION=$(node -p \"require('./package.json').version\") && ssh vertel@stage.kost.tv \"rm ~/stage.static.kost.tv/public_html/current && ln -s ~/stage.static.kost.tv/varta/releases/$VERSION/ ~/stage.static.kost.tv/public_html/current\""
  },
  "dependencies": {
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-regenerator": "^7.25.9",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.26.0",
    "@sentry/browser": "^5.5.0",
    "@sentry/integrations": "^5.5.0",
    "autoprefixer": "^9.5.1",
    "axios": "^1.7.7",
    "babel-loader": "^8.0.6",
