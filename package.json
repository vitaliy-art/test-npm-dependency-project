    "cross-env-file": "^1.0.0",
    "css-loader": "^2.1.1",
    "cypress": "^4.12.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "filemanager-webpack-plugin": "^3.1.1",
    "fs": "0.0.1-security",
    "graceful-fs": "^4.2.11",
    "gulp": "^4.0.2",
    "gulp-chmod": "^2.0.0",
    "gulp-debug": "^3.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.1.4",
    "intersection-observer-polyfill": "^0.1.0",
    "js-cookie": "^3.0.5",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "less": "^3.9.0",
    "mkdirp": "^1.0.4",
    "mocha": "^10.2.0",
    "mochapack": "^2.1.4",
    "navigator.sendbeacon": "0.0.20",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "postcss-less-loader": "^0.1.0",
    "raw-loader": "^2.0.0",
    "reset-css": "^4.0.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.9.0",
    "through2": "^4.0.2",
    "url-search-params-polyfill": "^5.1.0",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.16",
    "wait-on": "^3.2.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": {
    "production": [
      "> 0.5% in RU",
      "last 2 versions",
      "Firefox ESR",
      "not dead"
    ],
    "development": [
      "last 2 versions"
    ]
  },
  "main": ".eslintrc.js",
  "directories": {
    "doc": "docs"
  },
  "keywords": [],
